{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "zeek",
      "camelcase": "Zeek",
      "title": "Zeek",
      "scope": "source.zeek",
      "file-types": ["zeek"],
      "injection-regex": "^zeek$",
      "class-name": "TreeSitterZeek"
    }
  ],
  "metadata": {
    "version": "0.2.3",
    "license": "MIT",
    "description": "Zeek grammar for tree-sitter",
    "authors": [
      {
        "name": "The Zeek Project",
        "email": "info@zeek.org"
      }
    ],
    "links": {
      "repository": "https://github.com/zeek/tree-sitter-zeek"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
